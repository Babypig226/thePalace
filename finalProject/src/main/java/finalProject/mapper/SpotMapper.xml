<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace = "finalProject.mapper.SpotMapper">
	<insert id="insertSpot" parameterType="finalProject.domain.SpotDTO">
		insert into spot(spot_no, document_no, contract_no)
		values(#{spotNo}, #{documentNo}, #{contractNo}) 		
	</insert>

</mapper>